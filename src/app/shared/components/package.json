{
  "name": "ui-kit",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "b-kit": "npm run b-sass && ng build uikit && npm run b-sass-watch",
    "b-sass": "sass projects/uikit/src/styles/styles.scss dist/uikit/src/assets/styles.min.css --style=compressed",
    "b-sass-watch": "sass --watch projects/uikit/src/styles/styles.scss dist/uikit/src/assets/styles.min.css --style=compressed",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.9",
    "@angular/compiler": "^20.3.9",
    "@angular/core": "^20.3.9",
    "@angular/forms": "^20.3.9",
    "@angular/platform-browser": "^20.3.9",
    "@angular/platform-browser-dynamic": "^20.3.9",
    "@angular/router": "^20.3.9",
    "@bluehalo/ngx-leaflet": "^20.0.0",
    "@primeuix/themes": "^1.2.5",
    "leaflet": "^1.9.4",
    "leaflet-geosearch": "^4.2.2",
    "leaflet.fullscreen": "^4.0.0",
    "leaflet.locatecontrol": "^0.85.1",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "zone.js": "~0.15.0"
  },
