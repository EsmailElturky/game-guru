    "@types/leaflet-fullscreen": "^1.0.10",
    "@types/leaflet-geosearch": "^2.7.1",
    "@types/leaflet.locatecontrol": "^0.82.1",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.0",
